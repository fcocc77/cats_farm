[Unit]
Description=Service VinaRender Manager
After=network.target

[Service]
Type=forking
ExecStart=/bin/sh -c "{{path}}/os/linux/init/service.sh start vmanager {{port}}"
ExecStop=/bin/sh -c "{{path}}/os/linux/init/service.sh stop vmanager {{port}}"
Restart=always
TimeoutStartSec=0

[Install]
WantedBy=default.target
