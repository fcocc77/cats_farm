<widget class="QDockWidget" name="dockWidget">
    <property name="windowTitle">
        <string>Tasks</string>
    </property>
    <!-- el numero 2 significa que el QDockWidget se situa en la parte derecha -->
    <attribute name="dockWidgetArea">
        <number>2</number>
    </attribute>
    <!-- -->
    <widget class="QWidget" name="dockWidgetContents">
        <layout class="QVBoxLayout" name="verticalLayout_2">
            <item>
                <widget class="QTreeWidget" name="tasks">
                    {% include 'column.xml' with value="Task"%}
                    {% include 'column.xml' with value="Frame Range"%}
                    {% include 'column.xml' with value="Status"%}
                    {% include 'column.xml' with value="Server"%}
                    {% include 'column.xml' with value="Time"%}
                </widget>
            </item>
        </layout>
    </widget>
</widget>